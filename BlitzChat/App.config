      </setting>
      <setting name="TwitchSmiles" serializeAs="String">
        <value>https://api.twitch.tv/kraken/chat/emoticons</value>
      </setting>
      <setting name="SC2TV" serializeAs="String">
        <value>http://chat.sc2tv.ru/memfs/channel-{0}.json</value>
      </setting>
      <setting name="Cybergame" serializeAs="String">
        <value>ws://cybergame.tv:9090/websocket</value>
      </setting>
      <setting name="Goodgame" serializeAs="String">
        <value>ws://chat.goodgame.ru:8081/chat/websocket</value>
      </setting>
      <setting name="SC2TVSmiles" serializeAs="String">
        <value>http://chat.sc2tv.ru/js/smiles.js</value>
      </setting>
      <setting name="TwitchViewers" serializeAs="String">
        <value>https://api.twitch.tv/kraken/streams/</value>
      </setting>
    </BlitzChat.Settings.URLs>
  </userSettings>
</configuration>